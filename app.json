{
  "expo": {
    "name": "Sudoku",
    "slug": "sudoku",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sudoku",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#F8FAFC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sudoku.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F8FAFC"
      },
      "package": "com.sudoku.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"]
  }
}
